{
  "build": {
    "beforeDevCommand": "pnpm dev:fe",
    "beforeBuildCommand": "pnpm build:fe",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "hellebore",
  "mainBinaryName": "hellebore",
  "version": "0.0.0",
  "identifier": "com.hellebore.dev",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Hellebore",
        "maximized": true,
        "minWidth": 300,
        "minHeight": 500,
        "dragDropEnabled": false,
        "useHttpsScheme": true
      }
    ]
  }
}